<launch>

 <node pkg="rs_imu" type="rs_imu_node" name="rs_imu_node" required="true" output="screen" > 

    <!--param name="port" value="/dev/rs_imu" /-->
    <param name="port" value="/dev/ttyIMU" />
    <param name="imu_baudrate" value="115200" />
    <param name="imu_frame_id" value="imu_link" />
    <!--param name="imu_topic" value="imu_data" /-->
    <param name="imu_topic" value="imu" />
 </node>
 
 <!--node pkg="tf" type="static_transform_publisher" name="imu_broadcaster" args="0.20 0.0 0.40 0 0 0 base_link imu_link 100" respawn="true" /-->
 <node pkg="tf" type="static_transform_publisher" name="imu_broadcaster" args="-0.22 -0.13 0 0.5 0 0 base_link imu_link 100" respawn="true" />

</launch>
