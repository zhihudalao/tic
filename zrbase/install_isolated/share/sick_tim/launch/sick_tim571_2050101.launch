<launch>
  <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find sick_tim)/urdf/example.urdf.xacro'" />
  <!--node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher" /-->

  <node name="sick_tim571_2050101" pkg="sick_tim" type="sick_tim551_2050001" respawn="true" output="screen">
    <!-- default values: -->
    
      <!-- param name="min_ang" type="double" value="-2.35619449019" /-->
      <!-- param name="max_ang" type="double" value="2.35619449019" / -->
      <param name="min_ang" type="double" value="-1.74" />
      <param name="max_ang" type="double" value="1.74" />
      <param name="intensity" type="bool" value="True" />
      <param name="skip" type="int" value="0" />
      <param name="frame_id" type="str" value="laser" />
      <param name="time_offset" type="double" value="-0.001" />
      <param name="publish_datagram" type="bool" value="False" />
      <param name="subscribe_datagram" type="bool" value="false" />
      <param name="device_number" type="int" value="0" />
      <param name="range_min" type="double" value="0.05" />
    

    <param name="range_max" type="double" value="25.0" />
    <param name="hostname" type="string" value="192.168.8.100" />
    <param name="port" type="string" value="2112" />

    <param name="timelimit" type="int" value="5" />
    <!-- Older versions of the scanner firmware report an incorrect time_increment.
         If you get a warning about time_increment, uncomment this. -->
    <!-- <param name="time_increment" type="double" value="0.000061722" /> -->

  
  </node>

   <!--node pkg="tf" type="static_transform_publisher" name="base_link_to_laser"
    args="0.62 -0.05 0.22 0.0 0.0 0.0 base_link laser 100" /-->


   <node pkg="tf" type="static_transform_publisher" name="base_link_to_laser"
    args="0.624 0.0 0.20  0.0 0.0 -3.1415926 base_link laser 100" />
   

  <include file="/root/zrbase/install_isolated/share/sick_tim/launch/laser_filter.launch">
</include>

</launch>
