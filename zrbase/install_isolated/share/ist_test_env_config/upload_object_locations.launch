<?xml version="1.0"?>
<launch>

	<arg name="robot_env" default="ist-apartment"/>

	<!-- send objects locations to parameter server -->
	<!-- <rosparam command="delete" param="/objects"/> -->
	<rosparam command="load" ns="objects" file="$(find ist_test_env_config)/envs/$(arg robot_env)/object_locations.yaml"/>
	<rosparam command="load" ns="object_groups" file="$(find ist_test_env_config)/envs/$(arg robot_env)/object_groups.yaml"/>

</launch>
